{
  "name": "dev-duel",
  "main": "/server/index.js",
  "scripts": {
    "serve": "concurrently \"npm run serve-api\" \"npm run serve-static\"",
    "serve-api": "nodemon -w ./server --exec \"babel-node server --presets es2015,stage-0\"",
    "serve-static": "live-server client --host=localhost --port=8080 --watch=./",
    "lint": "eslint server client"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.13.3",
    "cors": "^2.7.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.13.3",
    "express-validation": "^1.0.2",
    "http-status": "^1.2.0",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "joi": "^13.6.0",
    "live-server": "^1.2.0",
    "nodemon": "^1.9.2"
  }
}